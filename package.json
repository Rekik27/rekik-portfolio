{
  "name": "rekik-portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
